    <h3><strong>Latest</strong></h3>
    <% @properties.each do |property| %>
        <div class="col-4 p-5">
            <%= link_to (property_thumbnail(property)), property_path(property) %>
        
            <h3 class="h5 mt-3 "><strong><%= property.name %></strong></h3>
            <p><%= property.address %></p>
            <p><%= property.created_at %></p>
            <div class="amenities">
            <span><i data-feather="users"></i></span>
            <span><i data-feather="showers "></span>
            <span></span>
            </div>
            </div>
        <% end %>